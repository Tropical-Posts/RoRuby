function get(url) {
	return new Promise(resolve => {
		const backendRequest = async () => {
			resolveOrTimeout(new Promise(resolve2 => {chrome.runtime.sendMessage({ greeting: "GetURL", url: url }, resolve2)}), 1000, "timeout").then(async (res) => {
				if (res != "timeout") {
					resolve(res)
				} else {
					const foundUrl = backendUrls.find(u => url.toLowerCase().includes(u))
					const func = alternativeUrls[backendUrls.indexOf(foundUrl)]
					if (func) {
						resolve(await resolveOrTimeout(func(url), 1000))
					} else {
						resolve()
					}
				}
			})
		}
		if (backendUrls.some(u => url.toLowerCase().includes(u))) {
			backendRequest()
			return
		}
		fetch(url, {mode:"cors", credentials:(url.includes("roblox") ? "include" : "same-origin")}).then((resp) => {
			resp.json().then(resolve)
		}).catch(() => {
			backendRequest()
		})
		// .catch(() => {
		// 	fetch(url).then(res => res.json()).then(res => resolve(res))
		// })
	})
}

const getClient = async (url) => {
	return new Promise(resolve => {
		fetch(url, {mode:"cors"}).then((resp) => {
			resp.json().then(resolve)
		}).catch((e) => {
			resolve(e)
		})
	})
}

const resolveOrTimeout = (promise, timeout, timeoutMessage) => {
	return new Promise((resolve, reject) => {
		setTimeout(() => {
			resolve(timeoutMessage)
		}, timeout)
		promise.then(resolve).catch(reject)
	})
}

